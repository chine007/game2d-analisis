<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="features.model" default-access="field">
	<class name="TestGameFeature" table="test_gamefeature">
		<id name="id">
			<generator class="identity"/>
		</id>
		<many-to-one name="user" class="User" column="username"/>
		<many-to-one name="game" class="Game" column="id_game"/>
		<many-to-one name="feature" class="Feature" column="id_feature"/>
		<property name="value"/>
	</class>
</hibernate-mapping>
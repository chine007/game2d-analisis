<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="features.persistence.model" default-access="field">
	<class name="GameCategory">
		<id name="id">
			<generator class="identity"/>
		</id>
		<property name="code"/>
		<set name="features" table="gamecategory_feature">
			<key column="id_gamecategory"/>
			<many-to-many class="Feature" column="id_feature"/>
		</set>
	</class>
</hibernate-mapping>